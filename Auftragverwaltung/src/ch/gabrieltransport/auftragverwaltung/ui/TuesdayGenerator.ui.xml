<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?import com.xdev.ui.XdevButton?>
<?import com.xdev.ui.XdevHorizontalLayout?>
<?import com.xdev.ui.XdevHorizontalSplitPanel?>
<?import com.xdev.ui.XdevTextField?>
<?import com.xdev.ui.XdevVerticalLayout?>
<XdevHorizontalLayout xmlns:x="http://xml.xdev-software.com/xdevuiml" spacing="false" x:name="TuesdayGenerator" margin="">
    <x:constraints designWidth="200" zpos="0" designHeight="92" width="200px" height="92px" />
    <x:event>layoutClick.layoutClick</x:event>
    <XdevHorizontalLayout x:name="horizontalLayout" margin="">
        <x:constraints weightx="0.1" width="100%" height="92px" />
        <XdevHorizontalSplitPanel x:name="horizontalSplitPanel" readOnly="true" splitPosition="100.0">
            <x:constraints weightx="1.0" width="100%" height="100%" />
            <XdevVerticalLayout x:name="verticalLayout" margin="">
                <x:constraints splitPos="FIRST" weightx="0.1" width="100%" alignment="TOP_CENTER" height="90px" />
                <XdevTextField columns="1" x:name="textField" value="Auftrag 1" x:styles="FontAwesome,borderless" readOnly="true">
                    <x:constraints weighty="0.0" weightx="0.1" width="100%" />
                </XdevTextField>
                <XdevTextField columns="1" x:name="textField2" value="Auftrag 2" x:styles="borderless" readOnly="true">
                    <x:constraints weighty="0.0" weightx="0.0" width="100%" />
                </XdevTextField>
                <XdevButton caption="" x:name="button" x:styles="borderless">
                    <x:constraints weighty="0.0" width="100%" alignment="MIDDLE_LEFT" />
                </XdevButton>
            </XdevVerticalLayout>
            <XdevVerticalLayout x:name="verticalLayout2" margin="">
                <x:constraints splitPos="SECOND" weightx="0.1" width="100%" alignment="TOP_CENTER" height="90px" />
                <XdevTextField columns="1" x:name="textField3" value="Auftrag Nachmittag" x:styles="FontAwesome,borderless,popupContent" readOnly="true">
                    <x:constraints weighty="0.0" weightx="0.1" width="100%" />
                </XdevTextField>
                <XdevTextField columns="1" x:name="textField4" value="Auftrag 2" x:styles="borderless" readOnly="true">
                    <x:constraints weighty="0.0" weightx="0.0" width="100%" />
                </XdevTextField>
                <XdevButton caption="" x:name="button2" x:styles="borderless">
                    <x:constraints weighty="0.0" width="100%" alignment="MIDDLE_LEFT" />
                </XdevButton>
            </XdevVerticalLayout>
        </XdevHorizontalSplitPanel>
    </XdevHorizontalLayout>
</XdevHorizontalLayout>

